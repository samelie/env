{
  "name": "@rad/env",
  "type": "module",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "module": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "dependencies": {
    "dotenv-flow": "^4.1.0"
  },
  "scripts": {
    "build": "tsup ./src --outDir dist --clean --format esm,cjs --shims --dts --platform node",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "types": "tsc -p tsconfig.typecheck.json",
    "knip": "knip"
  },
  "devDependencies": {
    "@rad/config": "workspace:*",
    "@rad/eslint": "workspace:*",
    "@types/dotenv-flow": "^3.3.3",
    "eslint": "^9.36.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
