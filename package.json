{
  "name": "@rad/env",
  "type": "module",
  "main": "./build/main.js",
  "module": "./build/main.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./build/main.d.ts",
      "import": "./build/main.js",
      "module": "./build/main.js",
      "require": "./build/main.cjs"
    }
  },
  "dependencies": {
    "dotenv-flow": "^4.1.0"
  },
  "scripts": {
    "build": "tsup ./src --outDir build --clean --format esm,cjs --shims --dts --platform node",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "types": "tsc -p tsconfig.typecheck.json"
  },
  "devDependencies": {
    "@rad/eslint": "workspace:*",
    "@types/dotenv-flow": "^3.3.3",
    "eslint": "^9.25.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
